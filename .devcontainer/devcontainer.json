{
    "name": "Python Development",
    "image": "mcr.microsoft.com/devcontainers/python:3.9",
  
    "customizations": {
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-toolsai.jupyter"
        ]
      }
    },
  
    "postCreateCommand": "pip install -r requirements.txt",
    
    "forwardPorts": [8888, 8080],
  
    "remoteUser": "vscode"
  }
  